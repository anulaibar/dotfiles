[alias]
	co = checkout
	br = branch
	ci = commit
	last = log -1 HEAD
	st = status
	unstage = reset HEAD --
[core]
	editor = /usr/local/bin/nvim
	excludesfile = ~/.gitignore
[push]
	default = simple
[user]
	name = Olle Hellgren
	email = olle.hellgren@blocket.se
[color]
	diff = auto
	status = auto
	branch = auto
[pull]
	rebase = true
[url "git@github.com:"]
	insteadOf = https://github.com/
